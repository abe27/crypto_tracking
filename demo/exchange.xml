<odoo>
    <data noupdate="1">
        <!-- Exchange List -->
        <record model="crypto_tracking.exchange_list">
            <field name="name">Bitkub</field>
            <field name="description">-</field>
            <field name="api_url">https://api.bitkub.com/api</field>
            <field name="api_key">Token</field>
            <field name="api_secret">Key</field>
            <field name="api_passphrase"></field>
            <field name="exchange_logo" type="base64" file="crypto_tracking/static/images/exchange/bitkub.png" />
            <field name="is_active">True</field>
        </record>

        <record model="crypto_tracking.exchange_list">
            <field name="name">Satang Pro</field>
            <field name="description">-</field>
            <field name="api_url">https://satangcorp.com/api</field>
            <field name="api_key">Token</field>
            <field name="api_secret">Key</field>
            <field name="api_passphrase"></field>
            <field name="exchange_logo" type="base64" file="crypto_tracking/static/images/exchange/satang_pro.jpeg" />
            <field name="is_active">True</field>
        </record>

        <record model="crypto_tracking.exchange_list">
            <field name="name">Binance</field>
            <field name="description">-</field>
            <field name="api_url">https://api.binance.com/api/v3</field>
            <field name="api_key">Token</field>
            <field name="api_secret">Key</field>
            <field name="api_passphrase"></field>
            <field name="exchange_logo" type="base64" file="crypto_tracking/static/images/exchange/binance.png" />
            <field name="is_active">True</field>
        </record>

        <record model="crypto_tracking.exchange_list">
            <field name="name">Kucoin</field>
            <field name="description">-</field>
            <field name="api_url">https://api.kucoin.com/api/v1</field>
            <field name="api_key">Token</field>
            <field name="api_secret">Key</field>
            <field name="api_passphrase"></field>
            <field name="exchange_logo" type="base64" file="crypto_tracking/static/images/exchange/kucoin.png" />
            <field name="is_active">True</field>
        </record>
    </data>
</odoo>