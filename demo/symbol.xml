        <!-- ดึงข้อมูล Reference -->
        <record id="dep_ps" model="crypto_tracking.symbol_group">
            <field name="name">Unknow</field>
        </record>
        <record model="crypto_tracking.symbol_list">
            <!-- field ที่เป็น Reference -->
